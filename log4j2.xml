<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</Console>
		<!-- Define el Appender aquí -->
	</Appenders>
	<Loggers>
		<Root level="error"/>
		<!-- Define el logger aquí -->
	</Loggers>
</Configuration>